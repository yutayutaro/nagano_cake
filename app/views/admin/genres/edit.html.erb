<h2 class= "bg-light">ジャンル編集</h2>
<%= form_with model: @genre, url:admin_genre_path, method: :patch do |f| %>
<table>
  <thead>
    <tr>
      <th>ジャンル名</th>
      <th><%= f.text_field :name %></th>
      <th><%= f.submit '変更を保存', class: "btn btn-success" %></th>
      <% end %>
    </tr>
  </thead>
 </table>